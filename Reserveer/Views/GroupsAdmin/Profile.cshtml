<script type="text/javascript">
    $(document).ready(function () {

        GroupProfileArray = new Array();
      GroupProfileArray = @Html.Raw(ViewData["results"]);
       
      
      
      $('#title').text(GroupProfileArray[0][1]);
      $('#groupI').val(GroupProfileArray[0][1]);
      $('#domainI').append('<h4 id="' + GroupProfileArray[0][4] + '" class="card-title text-center themeColor">' + GroupProfileArray[0][2]+'</h4> ');
      GroupRoomsArray = new Array();
      GroupRoomsArray = @Html.Raw(ViewData["results2"]); 
      var arrayLength = GroupRoomsArray.length;
        for (var i = 0; i < arrayLength; i++) {
            $("#roomsI").append(

              '<div class="col-sm-4 thumbnail"><div class="col-sm-7" ><h4 class="card-title text-center themeColor">'+GroupRoomsArray[i][1]+'</h4></div ><div class="col-sm-5"><a href="roomprofile?RoomId='+GroupRoomsArray[i][0]+'" class="btn btn-lg btn-danger btn-block  center-block">Edit</a></div></div >'
            );
      }

      GroupReservationsArray = new Array();
      GroupReservationsArray = @Html.Raw(ViewData["results4"]);
      var arrayLength = GroupReservationsArray.length;
      for (var i = 0; i < arrayLength; i++) {
        $("#reservationI").append(

          '<div class="row thumbnail" style="margin-right: 15px; margin-left: 15px;"><div class= "caption" ><div class= "col-sm-3" > ' + GroupReservationsArray[i][1] + '</div > <div class="col-sm-3">' + GroupReservationsArray[i][2] + ' - ' + GroupReservationsArray[i][3] + '</div><div class="col-sm-2">' + GroupReservationsArray[i][4] + '</div><div class="col-sm-2">' + GroupReservationsArray[i][7] + '</div><div class="col-sm-2"><a href="' + GroupReservationsArray[i][0] +'" class=" btn btn-sm btn-danger btn-block  center-block">DELETE</a></div></div ></div > '
        );
      }
      GroupUserArray = new Array();
      GroupUserArray = @Html.Raw(ViewData["results3"]);
      var arrayLength = GroupUserArray.length;
      for (var i = 0; i < arrayLength; i++) {
        $("#userI").append(

          '<div class="row thumbnail" style="margin-right: 15px; margin-left: 15px;"><div class= "caption" > <div class="col-sm-3">'+GroupUserArray[i][1]+'</div><div class="col-sm-3">'+GroupUserArray[i][2]+'</div><div class="col-sm-3">'+GroupUserArray[i][3]+'</div><div class="col-sm-3"><a href="'+GroupUserArray[i][3]+'" class="btn btn-sm btn-danger btn-block  center-block">Deactivate</a></div></div ></div >'
        );
      }



    });

</script>
@{
    ViewData["Title"] = "Profile";
}

<h1 class="text-center">Profile <span id="title"></span></h1>

<div class="panel-group" id="Group" role="tablist" aria-multiselectable="true">
    <div class="panel panel-default opacityWhitebg">
        <div class="panel-heading themeBGColor cwhite" role="tab" id="headingOne">
            <h4 class="panel-title">
                <a role="button" data-toggle="collapse" data-parent="#Group" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                    Group Info
                </a>
            </h4>
        </div>
        <div id="collapseOne" class="panel-collapse collapse in " style="padding:40px;" role="tabpanel" aria-labelledby="headingOne">

            <div class="row">

                <div class="col-sm-4">
                    <div class="row">
                        <div>
                            <img class="card-img-top" src="https://dummyimage.com/300x200/000/17a2b8 " alt="Card image cap" 
                                 style="height: 200px;  display: block; border: 2px #27A2A7 solid; padding: 2px;">
                        </div>
                    </div>
                    <div class="row">
                        <label class="radio-inline"><input type="radio" name="optradio" class="selected active" selected="selected">Active</label>
                        <label class="radio-inline"><input type="radio" name="optradio">Inactive</label>
                    </div>

                </div>
                <div class="col-sm-8">
                  <div class="input-group ">
                    <span class="input-group-addon" id="basic-addon1">Group Name</span>
                    <input id="groupI" type="text" class="form-control" placeholder="Group Name" aria-describedby="basic-addon1">
                  </div>
                  <button type="submit" class="btn btn-lg btn-primary btn-block  center-block" style="margin-top:10px;">Save</button>
                  <br />
                  <div id="domainI" class="col-sm-4 thumbnail"> 
                      

                  </div>
                </div>


            </div>

        </div>
    </div>
</div>



<div class="panel-group" id="Rooms" role="tablist" aria-multiselectable="true">
    <div class="panel panel-default opacityWhitebg">
        <div class="panel-heading themeBGColor cwhite" role="tab" id="heading3">
            <h4 class="panel-title">
                <a role="button" data-toggle="collapse" data-parent="#Rooms" href="#collapse3" aria-expanded="true" aria-controls="collapse3">
                    Rooms
                </a>
            </h4>
        </div>
        <div id="collapse3" class="panel-collapse collapse out " style="padding:20px;" role="tabpanel" aria-labelledby="heading3">
            <div class="row">
                <ul id="roomsI">
                    
                </ul>
            </div>

            <hr />

            <button type="submit" class="btn btn-lg btn-danger btn-block  center-block">Add Room</button>
        </div>
    </div>
</div>


